

<%= form_for @store do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :location %>
  <%= f.text_field :location %>

  <%= f.label :img_url %>
  <%= f.text_field :img_url %>

  <%= f.submit %>
<% end %>


<%= form_for @store, method: :delete do |f| %>
  <%= f.submit "Delete", class: "delete" %>
<% end %>
