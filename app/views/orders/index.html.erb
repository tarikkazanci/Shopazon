<h1>My Orders</h1>

<% @orders.each do |order| %>

<h3> Name: <%= order.product.name %></h3>
<h3> <%= order.created_at.strftime("%m/%d/%Y  %I:%M")  %></h3>
<h3> <%= link_to "View Order", order_path(order) %> </h3>
<h3> <%= link_to "Edit Order", edit_order_path(order) %> </h3>

<br>

<% end %>
